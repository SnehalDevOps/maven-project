pipeline {
agent any
stages {
stage ('SCM Checkout'){
git 'https://github.com/vpbobade/maven-project.git'
}
}
{
stage ('Compile Stage') {
agent { label 'slave1'}
steps {
withAnt(installation: 'Version 1.9.2', jdk: 'java-1.8.0-openjdk-1.8.0.201.b09-0.amzn2.x86_64') {
sh 'ant build service'
}
}
}
}
}
