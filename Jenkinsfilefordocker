node{
stage ('scm checkout') {
git ('https://github.com/vpbobade/maven-project')
}
stage ('Checkout to different branch') {
sh "git branch -r"
sh "git checkout master"
}
stage ('package stage') {
sh label: '', script: 'mvn clean package '
}
stage ('docker image build') {
sh 'docker build -t vbob/vbob-app:1.0.0 .'
}
stage ('Push Docker image to DockerHub') {
withCredentials([usernameColonPassword(credentialsId: 'dockerHubAccount', variable: 'credentials')]) {
sh "docker login -u vpbobade -p ${credentials}"
}
sh 'docker push vbob/vbob-app:1.0.0'
}
}
